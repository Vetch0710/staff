    <delete id="deleteStaffById" parameterType="String">
        delete
        from sys_staff
        where ID = #{userId}
    </delete>

    <delete id="deleteStaffByIds" parameterType="String">
        delete from sys_staff where ID in
        <foreach collection="array" item="userId" open="(" separator="," close=")">
            #{userId}
        </foreach>
    </delete>

