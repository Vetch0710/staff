    <update id="updateStaff" parameterType="SysStaff">
        update sys_staff
        <set>
            NAME = #{userName},
            SEX = #{sex},
            AGE = #{age},
            BIRTH_DAY = #{birthDay},
            PHONE = #{phone},
            NATION = #{nation},
            MARITAL = #{marital},
            WORK_LEVEL = #{workLevel},
            PROJECT_TEAM = #{projectTeam},
            DEPARTMENT = #{department},
            LEADER = #{leader},
            ISFINISH = #{isFinish},
            HOMETOWN = #{hometown},
            EDU_BACKGROUND = #{eduBackground},
            SCHOOL = #{school},
            MAJOR = #{major},
            GRADUATION_DAY = #{graduationDay},
            WORK_DAY = #{workDay},
            IDCARD_FRONT = #{idCardFront},
            IDCARD_BACK = #{idCardBack},
            DIPLOMA = #{diploma},
            DEGREE = #{degree}
        </set>

        where ID = #{userId}
    </update>
